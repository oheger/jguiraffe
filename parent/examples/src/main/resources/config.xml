<configuration>
    <system config-name="system-config"/>
	<xml config-name="userConfig" fileName="${user.home}/.jguiraffe-testappconfig.xml"
      config-optional="true" config-forceCreate="true"/>
	<additional>
		<xml fileName="framework-config.xml"/>
	</additional>
</configuration>