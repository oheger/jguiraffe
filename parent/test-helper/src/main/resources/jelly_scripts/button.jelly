<!--
  Jelly test script for creating buttons.
  $Id: button.jelly 205 2012-01-29 18:29:57Z oheger $
-->
<j:jelly xmlns:j="jelly:core" xmlns:f="formBuilder">
  <f:button text="Hello world!" foreColor="blue" mnemonic="w" command="Hello" name="Hello"/>
  <f:button backColor="black" alignment="center" command="Image" name="image">
    <f:icon resource="icon.gif"/>
  </f:button>
  <f:button textres="BTN_OK" resgrp="testformbuilderresources" command="OK"
    noField="false" name="ok" default="true"/>
  <f:button textres="BTN_CANCEL" mnemonicres="BTN_CANCEL" command="CANCEL" name="cancel"
    cancel="true"/>

  <f:toggleButton name="toggle" text="Toggle" command="tog"/>

  <f:button text="won't be created!" ifName="unexisting Builder" name="null"/>

  <!-- Undefined text and icon -->
  <f:button ifName="ERROR_BUILDER" name="error"/>
</j:jelly>
