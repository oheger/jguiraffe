<!--
  Jelly test script that defines multiple windows. This script is used for
  testing whether multiple named windows can be accessed.
  $Id: multiwindows.jelly 205 2012-01-29 18:29:57Z oheger $
-->
<j:jelly xmlns:j="jelly:core" xmlns:f="formBuilder" xmlns:a="actionBuilder"
  xmlns:w="windowBuilder">

  <!-- A helper dialog used by the frame -->
  <w:dialog name="msgDialog">
    <f:button name="dlgOkButton" text="A button"/>
  </w:dialog>

  <!-- The main frame -->
  <w:frame title="Main frame">
    <f:label name="label" text="A label"/>
  </w:frame>
</j:jelly>
